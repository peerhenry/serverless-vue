{
  "name": "serverless-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:all": "npm run build  && npm run lambda-build",
    "test": "vue-cli-service test:unit",
    "lint": "npm run lint:js && npm run lint:gql",
    "lint:js": "vue-cli-service lint",
    "lint:gql": "eslint src --ext .gql",
    "cy": "vue-cli-service test:e2e --port 3050 --mode test",
    "cypress": "vue-cli-service test:e2e --mode test",
    "dev": "concurrently --kill-others \"npm run lambda-serve\" \"npm run serve\"",
    "lambda-build": "netlify-lambda -c webpack.functions.js build src/lambda",
    "lambda-serve": "netlify-lambda -c webpack.functions.js serve src/lambda",
    "test:e2e": "vue-cli-service test:e2e --mode test",
    "shebang": "npm run build && npm run lint && npm run test && npm run cypress"
  },
  "dependencies": {
    "apollo-boost": "^0.4.3",
    "apollo-server-lambda": "^2.8.1",
    "auth0-js": "^9.11.1",
    "cache-loader": "^4.0.1",
    "core-js": "^2.6.5",
    "encoding": "^0.1.12",
    "graphql": "^14.4.2",
    "netlify-lambda": "^1.5.1",
    "vue": "^2.6.10",
    "vue-apollo": "^3.0.0-beta.11",
    "vue-router": "^3.0.3",
    "vuejs-logger": "1.5.3",
    "vuex": "^3.0.1",
    "ws": "^7.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-e2e-cypress": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-plugin-unit-jest": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "concurrently": "^4.1.1",
    "eslint": "^5.16.0",
    "eslint-plugin-graphql": "^3.0.3",
    "eslint-plugin-vue": "^5.0.0",
    "graphql-tag": "^2.9.0",
    "node-sass": "^7.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylint": "^2.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-apollo": "^0.21.0",
    "vue-cli-plugin-pug": "^1.0.7",
    "vue-template-compiler": "^2.6.10"
  }
}
